{
	"languageserver": {
		"go": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"trace.server": "verbose",
			"filetypes": ["go"]
		},
		"cpp": {
			"command": "ccls",
			"filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
			"rootPatterns": [".git",".ccls",".ccls-root", "build/compile_commands.json"],
			"initializationOptions": {
				"cache": {
					"directory": "/tmp/ccls-cache"
				},
				"client": {
					"snippetSupport": true
				},
				"compilationDatabaseDirectory": "build"
			}
		}
	},
	"coc.preferences.formatOnSaveFiletypes": ["go","rust"],
		"snippets.ultisnips.directories": [
		"UltiSnips",
		"~/.dotfiles/vim/mysnippets/"
	],
	"explorer.root.customRules": {
		"vcs": {
			"patterns": [".git", ".hg", ".projections.json"]
		},
		"vcs-r": {
			"patterns": [".git", ".hg", ".projections.json"],
			"bottomUp": true
		}
	},
	"rust-client.disableRustup":true
}
