{
	"languageserver": {
		"gopls": {
			"command": "gopls",
			"rootPatterns": ["go.mod",".vim/", ".git/", ".hg/"],
			"filetypes": ["go"]
		}
	},
	"snippets.ultisnips.directories": [
		"UltiSnips",
		"~/.dotfiles/vim/mysnippets/"
	],
	"explorer.root.customRules": {
		"vcs": {
			"patterns": [".git", ".hg", ".projections.json"]
		},
		"vcs-r": {
			"patterns": [".git", ".hg", ".projections.json"],
			"bottomUp": true
		}
	},
	"rust-client.disableRustup":true
}
